# ruby config
[ -f /usr/local/opt/chruby/share/chruby/chruby.sh ] && source /usr/local/opt/chruby/share/chruby/chruby.sh

# default to ruby 2 if chruby is around
command -v chruby >/dev/null 2>&1 && chruby ruby-2

# python config
export PYTHONPATH=\"$(brew --prefix)/lib/python2.7/site-packages:\$PYTHONPATH\"

# go config
# GOVERSION=$(brew list go | head -n 1 | cut -d '/' -f 6)
# export GOROOT=$(brew --prefix)/Cellar/go/$GOVERSION
# export PATH=$PATH:$GOROOT/bin
export GOPATH=$HOME/dev/lab/go # change this to whatever you want your GOPATH to be
